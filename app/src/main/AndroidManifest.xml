<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenUp"
        tools:targetApi="31">
        <activity
            android:name=".ui.IntroActivity"
            android:exported="false" />
        <activity
            android:name=".ui.mark.MarkActivity"
            android:exported="false" />
        <activity
            android:name=".ui.my.pwedit.PassCheckActivity"
            android:exported="false" />
        <activity
            android:name=".ui.my.meberedit.NameChangeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.mainnoti.Noti3Activity"
            android:exported="false" />
        <activity
            android:name=".ui.my.noti.Noti2Activity"
            android:exported="false" />
        <activity
            android:name=".ui.my.meberedit.MemberEditMainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.my.pwedit.PassEditMainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.main.FoodSearchResultActivity"
            android:exported="false" />

        <service
            android:name=".ui.my.MyFirebaseMessagingService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name=".ui.my.noti.NotiActivity"
            android:exported="false" />
        <activity
            android:name=".ui.my.MyMainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.signup.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".ui.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.viewpager.ViewPagerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.main.FoodSearchActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.main.FoodScanActivity"
            android:exported="false" />
        <activity
            android:name=".ui.main.MainActivity"
            android:exported="false" />
    </application>

</manifest>